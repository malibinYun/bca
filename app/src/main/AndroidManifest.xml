        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".movie.select.MovieSelectActivity"></activity>
        <activity android:name=".review.more.ReviewMoreActivity" />
        <activity android:name=".review.write.ReviewWriteActivity" />
